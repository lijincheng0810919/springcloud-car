                    if ($('#vcodeimg_l').next().hasClass('error') || $('#vcodeimg_l').next().hasClass('sure')) {
                        $('#vcodeimg_l').next().remove();
                    }
                    flush_vcode('#vcodeimg_l');
                });
            });
            $('#regA').click(function(){
