        });
        init_page_nav(old_cityname);
        typeof (init_pc_page) == 'function' && init_pc_page(TOP_INFO.location.ename);
        if (!$.isEmptyObject(TOP_INFO.user)) {
            //提示成功注册时，执行以下语句
            _hmt.push(['_trackEvent', 'Login', ' Login_auto']);
